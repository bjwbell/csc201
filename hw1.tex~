\section*{Problem 1}
\begin{enumerate}
\item square: $\mathbb{R} \rightarrow \mathbb{R}$.
\item absolute : $\mathbb{R} \rightarrow \mathbb{R}$.
\item ``+'': $\mathbb{R} \times \mathbb{R} \rightarrow \mathbb{R}$.
\item ``-'': $\mathbb{R} \times \mathbb{R} \rightarrow \mathbb{R}$.
\item ``*'': $\mathbb{R} \times \mathbb{R} \rightarrow \mathbb{R}$.
\item ``/'': $\mathbb{R} \times \mathbb{R'} \rightarrow \mathbb{R}$.
  
\end{enumerate}

\section*{Problem 2}
\begin{enumerate}
\item The signature of iterate is $\textrm{iterate} : (f:A \rightarrow A) \times \mathbb{N} \rightarrow A.$
\item The signature of curry(iterate is $\textrm{curry(iterate)} : (f: A \rightarrow A) \rightarrow (\mathbb{N} \rightarrow A).$
\end{enumerate}
