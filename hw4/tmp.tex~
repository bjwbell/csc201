\section*{Problem 1}
\begin{description} 
\item \{M1^A\} while C1 do \{C1^A\} (...) \{A^\!C1^M2\}
\item \{C1^A\} if C2 then \{C1^C2^A\} (...) else \{C1^A^\!C2\} L2 \{A\}
\item \{C1^C2^A\} (while C3 do \{C1^C2^A^C3\} L1) \{A^\!C3\}
\end{description}
